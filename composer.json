{
    "name": "kuttumiah/recaptcha",
    "description": "reCAPTCHA Validator for Laravel 5",
	"keywords": ["recaptcha", "captcha", "laravel", "laravel5"],
	"homepage": "http://github.com/kuttumiah/recaptcha",
	"license": "MIT",
    "authors": [
        {
            "name": "Md Robaiatul Islam",
            "email": "robaiat.shaon@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "illuminate/support": "~5.1|^6.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Kuttumiah\\Recaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kuttumiah\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "Kuttumiah\\Recaptcha\\Facades\\Recaptcha"
            }
        },
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev"
}
